<!-- pages/home/childComponents/w-recommend/w-recommend.wxml -->
<view class="recommend">
  <block wx:for="{{recommends}}" wx:key="index">
    <view class="recommend-item">
      <image class="recommend-image" src="{{item.image}}" bindload="handleBindLoad" />
      <view class="recommend-title">{{item.title}}</view>
    </view>
  </block>
</view>